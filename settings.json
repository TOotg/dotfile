{
  "editor.fontFamily": "Menlo,Cousine,Hannotate SC ",
  "editor.fontSize": 15,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.lineHeight": 21,
  "editor.snippetSuggestions": "top",
  "editor.minimap.enabled": false,
  "editor.cursorStyle": "block",
  "editor.wordWrap": "wordWrapColumn",
  "editor.formatOnSave": false,
  "editor.lineNumbers": "relative",
  "editor.codeLens": false,
  "editor.renderControlCharacters": true,
  "editor.minimap.renderCharacters": false,
  "editor.acceptSuggestionOnEnter": "off",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "files.insertFinalNewline": true,
  "files.autoSave": "off",
  "files.associations": {
    "*.wxml": "html",
    "*.wxss": "css"
  },
  "explorer.openEditors.visible": 5,
  "explorer.openEditors.dynamicHeight": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.showTabs": false,
  "workbench.editor.enablePreview": true,
  "workbench.statusBar.visible": true,
  "workbench.iconTheme": "file-icons-colourless",
  "workbench.activityBar.visible": false,
  "workbench.sideBar.location": "right",
  "window.zoomLevel": 0,
  "window.newWindowDimensions": "inherit",
  "window.nativeTabs": true,
  "search.followSymlinks": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "menlo",
  "projectManager.treeview.visible": false,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.autoImportSuggestions.enabled": false,
  "gitlens.blame.line.enabled": false,
  "gitlens.gitExplorer.enabled": false,
  "gitlens.statusBar.enabled": false,
  "zenMode.hideStatusBar": false,
  "eslint.alwaysShowStatus": true,
  "eslint.options": {
    "configFile": "/Users/tan/.vim/eslint.config.json"
  },
  "vim.useSystemClipboard": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors": {
    "normal": "#2B2D42",
    "insert": "#4C4C4C",
    "visual": "#00A896",
    "visualline": "#52B095",
    "visualblock": "#028090",
    "replace": "#000000"
  },
  "vim.visualstar": true,
  "vim.otherModesKeyBindingsNonRecursive": [
    {
      "before": [
        ";"
      ],
      "after": [
        ":"
      ]
    },
    {
      "before": [
        " "
      ],
      "after": [
        "/"
      ]
    },
    {
      "before": [
        "u"
      ],
      "commands": [
        {
          "command": "undo",
          "when": "editorTextFocus && !editorReadonly"
        }
      ]
    },
    {
      "before": [
        "U"
      ],
      "commands": [
        {
          "command": "editor.action.transformToUppercase"
        }
      ]
    },
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        {
          "command": "workbench.action.navigateForward"
        }
      ]
    },
    {
      "before": [
        "g",
        "o"
      ],
      "commands": [
        {
          "command": "workbench.action.navigateBack"
        }
      ]
    },
    {
      "before": [
        "g",
        "p"
      ],
      "commands": [
        {
          "command": "editor.action.previewDeclaration"
        }
      ]
    },
    {
      "before": [
        "g",
        "n"
      ],
      "commands": [
        {
          "command": "workbench.action.focusNextGroup"
        }
      ]
    },
    {
      "before": [
        "g",
        "j"
      ],
      "commands": [
        {
          "command": "workbench.action.openPreviousEditorFromHistory"
        }
      ]
    },
    {
      "before": [
        "g",
        "u"
      ],
      "commands": [
        {
          "command": "editor.action.goToDeclaration",
          "when": "editorTextFocus && !isInEmbeddedEditor"
        }
      ]
    },
    {
      "before": [
        "t",
        "p"
      ],
      "commands": [
        {
          "command": "eslint.executeAutofix"
        }
      ]
    }
  ],
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressUpdateNotice": true,
    "suppressWelcomeNotice": false
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.colorTheme": "Panda Syntax",
  "workbench.colorCustomizations": {
    "statusBar.background": "#52B095",
    "statusBar.noFolderBackground": "#52B095",
    "statusBar.debuggingBackground": "#52B095"
  }
}
